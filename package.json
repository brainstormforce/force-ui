    "lexical": "^0.17.0",
    "lucide-react": "^0.417.0",
    "nanoid": "^5.0.7",
    "react-day-picker": "^9.1.3",
    "styled-components": "^6.1.12",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-scoped-preflight": "^3.4.3"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/force-ui.js"
    },

    "@wordpress/eslint-plugin": "^20.3.0",
    "@wordpress/prettier-config": "^4.4.0",
    "@wordpress/scripts": "^27.9.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "chromatic": "^11.7.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8.4.39",
